{
  "name": "y",
  "version": "1.0.0",
  "description": "Test the binance API and its websockets",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test-dev": "jest --silent --detectOpenHandles --setupFilesAfterEnv ./test-env-setup.ts",
    "dev": "nodemon -e ts,ejs --exec npm run dev:start",
    "start": "pm2 restart all",
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc copy-assets",
    "copy-assets": "ts-node copyAssets",
    "prod": "npm-run-all build start",
    "first-start": "node ./dist/server.js",
    "deploy": "npm-run-all build first-start"
  },
  "author": "Florencia Petrikovich",
  "license": "ISC",
  "dependencies": {
    "@types/moment-timezone": "^0.5.30",
    "bunyan": "^1.8.15",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.5.1",
    "helmet": "^4.6.0",
    "nodemon": "^2.0.15"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/bunyan": "^1.8.7",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "ts-node": "^10.4.0"
  }
}
